<nav>
  <ol>
  {{#recursive .}}
    <li>
    {{#if id}}
      {{#if title}}
        <a href="{{id}}">{{title}}</a>
      {{else}}
        <a href="{{id}}" class="autogenerated-text">DEFAULT_TITLE</a>
      {{/if}}
    {{else}}
      <span>{{title}}</span>
    {{/if}}
    {{#if children}}
      <ol>
        {{{recursive children}}}
      </ol>
    {{/if}}
    </li>
  {{/recursive}}
  </ol>
</nav>
